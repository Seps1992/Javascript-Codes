function palindrome(str) {

var str = str.replace(/[^a-zA-Z0-9]+/gi, '').toLowerCase();
  return str == str.split('').reverse().join('');

for(var i = 0; i < Math.floor(str.length/2); i++) { 
    if(str(i) !== str(str.length-i-1)) { 
      return false;
    }
  }
  return true;
  
}



palindrome("eye");
